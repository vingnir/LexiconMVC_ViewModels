
@model List<PeopleViewModel>


<table class="table">
    <tr>
        <th>
            Id
        </th>
        <th>
            Name
        </th>

        <th>
            City
        </th>
    </tr>
    @foreach (var item in Model)
    {
<tr>
    <td id="name">
        @Html.DisplayFor(modelItem => item.Id)
    </td>
    <td id="name">
        @Html.DisplayFor(modelItem => item.Name)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.City)
    </td>

    <td class="text-dark">
        <i class="fa fa-edit"></i>@Html.ActionLink("Edit", "Edit", new { id = item.Id }) |
        <i class="fa fa-table"></i> @Html.ActionLink("Details", "Details", new { id = item.Id }) |
        <i class="fa fa-trash"></i> @Html.ActionLink("Delete", "Delete", new { id = item.Id })
    </td>
</tr>
    }

</table>
<br />
